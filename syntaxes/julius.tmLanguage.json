{
  "scopeName": "source.yesod.julius",
  "fileTypes": ["julius"],
  "name": "Yesod Julius",
  "patterns": [
    { "include": "source.js" },
    { "include": "#interpolation" }
  ],
  "repository" :{
    "interpolation": {
      "patterns": [
        {
          "match": "([#@]){(.+)}",
          "captures": {
            "1": {
              "name": "entity.other.attribute-name.yesod"
            },
            "2": {
              "name": "constant.other.yesod"
            }
          },
          "name": "interpolation.yesod"
        }
      ]
    }
  }
}